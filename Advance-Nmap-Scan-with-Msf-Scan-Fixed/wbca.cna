cmd($console, "db_nmap --min-hostgroup 96 -T4 -n -F $1");
println("\n---NMAP Scan Finished---\n");
}
item "Quick Scan (OS detect)" {
$console = open_console_tab("nmap");
cmd($console, "db_nmap --min-hostgroup 96 -sV -n -T4 -O -F --version-light $1");
