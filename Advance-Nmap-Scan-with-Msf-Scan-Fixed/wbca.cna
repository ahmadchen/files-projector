}
item "Comprehensive" {
$console = open_console_tab("nmap");
cmd($console, "db_nmap --min-hostgroup 96 -sS -n -sU -T4 -A -v -PE -PP -PS80,443 -PA3389 -PU40125 -PY -g 53 $1");
println("\n---NMAP Scan Finished---\n");
}
